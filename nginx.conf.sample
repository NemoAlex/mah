server {
    listen       80;
    server_name  latino.dev;
    root         /root/latino/build;

    gzip on;
    gzip_proxied any;
    gzip_types text/plain text/xml text/css application/x-javascript application/javascript application/octet-stream image/svg+xml;
    gzip_vary on;
    gzip_disable "MSIE [1-6]\.(?!.*SV1)";

    location / {
    }
}